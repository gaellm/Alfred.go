{
    "name": "plaintext-req-helpers",
    "request": {
        "method": "POST",
        "url": "/some/plaintext-req-helpers/:path-var"
    },
    "response": {
        "status": 200,
        "body": "{\"from-req-body\":\"{{ alfred.req.test.body-var @regex:'.*test=([^ ]*).*' }}\", \"from-req-path\":\"{{ alfred.req.path-var }}\"}",
        "headers": {
            "Content-Type": "application/json",
            "From-Req-Header": "{{ alfred.req.header-var }}"
        }
    }
}